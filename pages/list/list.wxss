.container{
  background-color: #f8f8f8;
  margin-bottom: 3%;
}
.head_space{
  margin:1% 0% 0% 0%;
}
.special_column{
  margin:0% 0% 5% 0%;
  padding:0% 3% 0% 3%;
  height:100%;
  float: left;
  width: 92vw;
  font-size:18px;
  line-height:180%;
  border-bottom:0.5px solid  #f8f8f8;
  background-color: #ffffff;
}
.article_list{
  line-height: 1.5rem;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-bottom:0.5px solid  #f8f8f8;
  padding: 0.8rem 1rem 0.8rem 1rem;
}
.column_head{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 92vw;
  line-height:3rem;
}
.head_right{
  font-size:1rem;
  color:#ccc
}
.end{
  font-size:12px;
  margin:0% 0% 30% 0%;
}
.footer_space{
  margin:0% 0% 20% 0%;
}

